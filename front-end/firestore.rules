rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // allow read, write;
      // allow read, create , delete;
      
      // only auth user can create
      allow create: if request.auth != null;
      
      // only auth user can create
      allow read, delete: if request.auth.uid == resource.data.uid;
    }
  }
}